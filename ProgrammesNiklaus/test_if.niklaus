// La compilation doit reussir

// Lit un entier n, puis imprime
//   10, 20, 30, 41, 51, 61  si   n < 7
//   10, 21, 31, 41, 50, 60  si   n = 7
//   11, 21, 30, 40, 50, 61  si   n > 8

program test_if;
var a, b, c, d;
{
	b := 7;
	read a;
	if ( b < a ) {
		write 11;
	} else {
		write 10;
	}
	if ( b <= a ) {
		write 21;
	} else {
		write 20;
	}
	if ( b = a ) {
		write 31;
	} else {
		write 30;
	}
	if ( b >= a ) {
		write 41;
	} else {
		write 40;
	}
	if ( b > a ) {
		write 51;
	} else {
		write 50;
	}
	if ( b <> a ) {
		write 61;
	} else {
		write 60;
	}
}

